  <managed-bean-name>authenticator</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.seguridad.Authenticator</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userExtBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.UserExtBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sincPermissionBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.SincPermissionBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/index.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>adminDomain</from-outcome>
   <to-view-id>/webContent/adminDomain/listAdmin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createDomain</from-outcome>
   <to-view-id>/webContent/adminDomain/createDomain.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>adminGroupRole</from-outcome>
   <to-view-id>/webContent/adminGroupRole/listAdmin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createGroupRole</from-outcome>
   <to-view-id>/webContent/adminGroupRole/createGroupRole.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createUser</from-outcome>
   <to-view-id>/webContent/admUser/createUser.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>adminGroupPermission</from-outcome>
   <to-view-id>/webContent/adminGroupPermission/listAdmin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createGroupPermission</from-outcome>
   <to-view-id>/webContent/adminGroupPermission/createGroupPermission.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>adminPermission</from-outcome>
   <to-view-id>/webContent/adminPermission/listAdmin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createPermission</from-outcome>
   <to-view-id>/webContent/adminPermission/createPermission.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>adminRole</from-outcome>
   <to-view-id>/webContent/adminRole/listAdmin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createRole</from-outcome>
   <to-view-id>/webContent/adminRole/createRole.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>userAdminDomain</from-outcome>
   <to-view-id>/webContent/adminDomain/addUserDomain.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addPermission</from-outcome>
   <to-view-id>/webContent/adminRole/addPermission.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addUsers</from-outcome>
   <to-view-id>/webContent/adminRole/addUser.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>depureUsers</from-outcome>
   <to-view-id>/webContent/admUser/depureUsers.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminRole/listAdmin.jsp</from-view-id>
  <navigation-case>
   <from-outcome>detalle</from-outcome>
   <to-view-id>/webContent/adminRole/admin.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>nuevo</from-outcome>
   <to-view-id>/webContent/adminRole/create.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminRole/create.jsp</from-view-id>
  <navigation-case>
   <from-outcome>regresar</from-outcome>
   <to-view-id>/webContent/adminRole/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminRole/admin.jsp</from-view-id>
  <navigation-case>
   <from-outcome>regresar</from-outcome>
   <to-view-id>/webContent/adminRole/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminDomain/listAdmin.jsp</from-view-id>
  <navigation-case>
   <from-outcome>updateDomain</from-outcome>
   <to-view-id>/webContent/adminDomain/updateDomain.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminDomain/updateDomain.jsp</from-view-id>
  <navigation-case>
   <from-outcome>return</from-outcome>
   <to-view-id>/webContent/adminDomain/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminGroupPermission/listAdmin.jsp</from-view-id>
