  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminGroupPermission/updateGroupPermission.jsp</from-view-id>
  <navigation-case>
   <from-outcome>return</from-outcome>
   <to-view-id>/webContent/adminGroupPermission/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminGroupRole/listAdmin.jsp</from-view-id>
  <navigation-case>
   <from-outcome>updateGroupRole</from-outcome>
   <to-view-id>/webContent/adminGroupRole/updateGroupRole.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminGroupRole/updateGroupRole.jsp</from-view-id>
  <navigation-case>
   <from-outcome>return</from-outcome>
   <to-view-id>/webContent/adminGroupRole/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminPermission/listAdmin.jsp</from-view-id>
  <navigation-case>
   <from-outcome>updatePermission</from-outcome>
   <to-view-id>/webContent/adminPermission/updatePermission.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/webContent/adminPermission/updatePermission.jsp</from-view-id>
  <navigation-case>
   <from-outcome>return</from-outcome>
   <to-view-id>/webContent/adminPermission/listAdmin.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- USUARIOS EXTERNOS  -->
 <navigation-rule>
  <navigation-case>
   <from-outcome>RegistrarCuenta</from-outcome>
   <to-view-id>/webContent/principal.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>estadoCuenta</from-outcome>
   <to-view-id>/webContent/principal.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- SINCRONIZACION DE PERMISOS DESDE OTROS AMBIENTE  -->
