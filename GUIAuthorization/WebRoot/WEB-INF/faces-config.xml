<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config>
 <managed-bean>
  <managed-bean-name>domainBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.DomainBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>permissionBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.PermissionBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>menuBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.MenuBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>roleBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.RoleBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.UserBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>groupRoleBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.GroupRoleBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>groupPermissionBean</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.GroupPermissionBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listadosGUIAuthorization</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.helper.ListadosGUIAuthorization</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>identity</managed-bean-name>
  <managed-bean-class>com.osi.gaudi.security.authorization.beans.IdentityBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
